<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextView.Cells.Title" parent="TextView">
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:lineHeight">@dimen/cells_title_line_height_20sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:layout_marginStart">@dimen/margin_start_8dp</item>
        <item name="android:textSize">@dimen/cells_title_text_size_17sp</item>
    </style>

    <style name="TextView.Cells.SubTitle" parent="TextView.Cells.Title">
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:lineHeight">@dimen/cells_title_line_height_16.41sp</item>
        <item name="android:textSize">@dimen/cells_title_text_size_16sp</item>
    </style>

    <style name="TextView.Cells.Details" parent="TextView.Cells.Title">
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:layout_marginStart">@dimen/margin_start_0dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_end_16dp</item>
    </style>

    <style name="TextView.Divider.Cells.Title" parent="TextView.Cells.Title">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:fontWeight">500</item>
        <item name="android:lineHeight">@dimen/cells_title_line_height_19sp</item>
    </style>

    <style name="TextView.Divider.Cells.Button" parent="TextView.Cells.Title">
        <item name="android:textColor">@color/blue_1</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="Divider">
        <item name="android:background">?android:attr/listDivider</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="Divider.Horizontal" parent="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/black_4</item>
    </style>

    <style name="Divider.Vertical" parent="Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="TextView.Cells.Title.Faq" parent="TextView">
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:layout_marginStart">@dimen/margin_start_16dp</item>
        <item name="android:textSize">@dimen/cells_title_text_size_17sp</item>
        <item name="android:letterSpacing">-0.0041</item>
        <item name="android:fontWeight">400</item>
        <item name="android:textColor">@color/white_1</item>
        <item name="android:lineHeight">@dimen/cells_title_line_height_20.4sp</item>
    </style>

    <style name="TextView.Cells.SubTitle.Faq" parent="TextView.Cells.SubTitle">
        <item name="android:layout_marginStart">@dimen/margin_start_16dp</item>
        <item name="android:textSize">@dimen/cells_subTitle_text_size_15sp</item>
        <item name="android:lineHeight">@dimen/cells_subTitle_line_height_17.9sp</item>
        <item name="android:textColor">@color/gray_1</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="TextView.Price.Verifying.Texts" parent="TextView.Cells.SubTitle.Faq">
        <item name="android:textColor">@color/gray_4</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
    </style>

    <style name="TextView.Cells.Info" parent="TextView">
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:layout_marginStart">@dimen/margin_end_8dp</item>
        <item name="android:textSize">@dimen/cells_info_text_size_15sp</item>
        <item name="android:lineHeight">@dimen/cells_info_line_height_17.9sp</item>
        <item name="android:fontWeight">400</item>
    </style>

    <style name="TextView.Price.WithoutCoins" parent="TextView.Price">
        <item name="android:textSize">@dimen/price_without_coins_text_size_22sp</item>
        <item name="android:lineHeight">@dimen/price_without_coins_line_height_26sp</item>
    </style>

    <style name="TextView.Price.WithCoins" parent="TextView.Price">
        <item name="android:textSize">@dimen/price_with_coins_text_size_14sp</item>
        <item name="android:lineHeight">@dimen/price_with_coins_line_height_17sp</item>
    </style>

    <style name="TextView.Price">
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:fontWeight">700</item>
    </style>

    <style name="EditableTextView">
        <item name="android:fontFamily">@font/sf_pro_text_semibold</item>
        <item name="android:minHeight">@dimen/min_height_20dp</item>
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:textSize">@dimen/text_17sp</item>
        <item name="android:fontWeight">600</item>
        <item name="android:lineHeight">@dimen/editable_text_line_height_20.29sp</item>
    </style>

    <style name="EditableTextView.Hint" parent="EditableTextView">
        <item name="android:textColorHint">@color/gray_1</item>
        <item name="android:alpha">0.5</item>
    </style>

    <style name="TextView.Telegram_Hint" parent="TextView.O_Money_Agreement">
        <item name="android:textColor">@color/gray_3</item>
    </style>

    <style name="EditableTextView.AsHint" parent="EditableTextView">
        <item name="android:textColor">@color/gray_1</item>
        <item name="android:alpha">0.5</item>
    </style>

    <style name="OTPInputText">
        <item name="android:inputType">number</item>
        <item name="android:maxLength">1</item>
        <item name="android:textColor">@color/white_1</item>
        <item name="android:textSize">@dimen/text_17sp</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:lineHeight">20.29dp</item>
        <item name="android:fontWeight">500</item>
    </style>

    <style name="Widget.Theme.OMarket.MyView" parent="">
        <item name="android:background">@color/gray_600</item>
    </style>

    <style name="AppTitleStyle">
        <item name="android:textColor">@color/white_1</item>
        <item name="android:textSize">@dimen/text_32sp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_6dp</item>
    </style>

    <style name="AppPrimaryStyle">
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_3dp</item>
    </style>

    <style name="AppRegStyle">
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/text_14sp</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_2dp</item>
    </style>

    <style name="AppTopScreenStyle">
        <item name="android:fontFamily">@font/sf_pro_text_semibold</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_3dp</item>
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textSize">@dimen/text_15sp</item>
    </style>

    <style name="AppDefaultBlueText">
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/text_15sp</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_3dp</item>
        <item name="android:textColor">@color/blue_1</item>
    </style>

    <style name="FirstPageTitle" parent="AppTitleStyle">
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
    </style>

    <style name="Error500Title" parent="AppTitleStyle">
        <item name="android:fontFamily">@font/sf_pro_display_regular</item>
    </style>

    <style name="FirstPagePrimaryText" parent="AppPrimaryStyle">
        <item name="android:textSize">@dimen/text_17sp</item>
        <item name="android:textColor">@color/white_1</item>
    </style>

    <style name="PrimaryUnderFieldText" parent="AppPrimaryStyle">
        <item name="android:textSize">@dimen/text_13sp</item>
        <item name="android:textColor">@color/gray_3</item>
    </style>

    <style name="TimerText" parent="AppPrimaryStyle">
        <item name="android:textSize">@dimen/text_15sp</item>
        <item name="android:textColor">@color/gray_3</item>
    </style>

    <style name="ForgetPasswordBlueText" parent="AppDefaultBlueText">
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="ErrorUnderInpText" parent="AppPrimaryStyle">
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_2dp</item>
        <item name="android:textSize">@dimen/text_14sp</item>
        <item name="android:textColor">@color/red_1</item>
    </style>

    <style name="LicenseRegularText" parent="AppRegStyle">
        <item name="android:textColor">@color/gray_3</item>
    </style>

    <style name="licenseTouchableText" parent="AppRegStyle">
        <item name="android:textColor">@color/magenta_1</item>
    </style>


    <style name="termsStyle">
        <item name="android:textColor">@color/white_1</item>
        <item name="android:textColorLink">@color/magenta_1</item>
        <item name="android:textSize">@dimen/text_14sp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:lineHeight">@dimen/line_16.41dp</item>
        <item name="android:fontWeight">400</item>
    </style>

    <style name="toolbarTitle">
        <item name="android:textSize">@dimen/text_15sp</item>
        <item name="android:fontFamily">@font/sf_pro_text_semibold</item>
        <item name="android:textColor">@color/white_1</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:fontWeight">600</item>
        <item name="android:lineHeight">@dimen/line_17.9dp</item>
    </style>

    <style name="hint">
        <item name="android:textSize">@dimen/text_13sp</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textColor">@color/white_1</item>
    </style>

    <style name="numberInput">
        <item name="android:textSize">@dimen/text_17sp</item>
        <item name="android:fontFamily">@font/sf_pro_text_semibold</item>
        <item name="android:textColor">@color/white_1</item>
    </style>

    <style name="SentSMSText" parent="AppPrimaryStyle">
        <item name="android:textSize">@dimen/text_15sp</item>
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_0dp</item>
        <item name="android:lineHeight">@dimen/line_17.9dp</item>
        <item name="android:fontWeight">400</item>
    </style>

    <style name="detailedAdsTitleStyle" parent="AppPrimaryStyle">
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:lineSpacingExtra">@dimen/extra_spacing_3dp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/white_1</item>
        <item name="android:layout_marginStart">@dimen/margin_16dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_16dp</item>
        <item name="android:layout_marginTop">@dimen/margin_16dp</item>
        <item name="android:layout_marginBottom">@dimen/margin_8dp</item>
        <item name="android:minLines">1</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
    </style>
</resources>